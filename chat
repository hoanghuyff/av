-- Script Auto Add Friend vÃ  Auto Accept Friend Requests
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- HÃ m tá»± Ä‘á»™ng gá»­i yÃªu cáº§u káº¿t báº¡n
local function autoSendFriendRequest()
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and not LocalPlayer:IsFriendsWith(player.UserId) then
            LocalPlayer:RequestFriendship(player)
            print("ÄÃ£ gá»­i yÃªu cáº§u káº¿t báº¡n tá»›i:", player.Name)
        end
    end
end

-- HÃ m tá»± Ä‘á»™ng cháº¥p nháº­n yÃªu cáº§u káº¿t báº¡n
local function autoAcceptFriendRequests()
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player:IsFriendRequestSent() then
            LocalPlayer:AcceptFriendRequest(player)
            print("ÄÃ£ cháº¥p nháº­n yÃªu cáº§u káº¿t báº¡n tá»«:", player.Name)
        end
    end
end

-- VÃ²ng láº·p gá»­i yÃªu cáº§u káº¿t báº¡n vÃ  cháº¥p nháº­n yÃªu cáº§u káº¿t báº¡n liÃªn tá»¥c
while true do
    autoSendFriendRequest()
    autoAcceptFriendRequests()
    task.wait(10) -- Láº·p láº¡i má»—i 10 giÃ¢y
end
