getgenv().petsGoConfig = {
    WEBHOOK_URL = "https://discord.com/api/webhooks/1304820433824256190/Rpf1gNmWZcG24jQXBy92k0lGWEfx7I4kBBhCwuTeNUaoKJBFDAE6xT-wURQXeh1idGjS",
    MAILING_WEBHOOK_URL = "",
    DISCORD_ID = "1120921298860773456",
    WEBHOOK_ODDS = 100000000, -- Minimum Pet Odds To Trigger Webhook
    MAIL_PET = true,  -- Mail Pet
    MAIL_PET_ODDS = 100000000,  -- Minimum Pet Odds To Mail
    USERNAME_TO_MAIL = "ShermanHarold8" -- Mail Pet To Username
}

local Players = game:GetService('Players')
local Client = Players.LocalPlayer

_G.EnableFriendRequest = true  -- Bật tính năng gửi yêu cầu kết bạn
_G.WaitToSendFriend = 300  -- Thời gian chờ giữa các lần gửi yêu cầu kết bạn

local function AutoAddFriends()
    while _G.EnableFriendRequest do
        for _, v in ipairs(Players:GetPlayers()) do
            if v ~= Client then
                Client:RequestFriendship(v)  -- Gửi yêu cầu kết bạn
                warn("Send request:", v.Name)
            end
            wait(1)  -- Thời gian chờ giữa các lần gửi yêu cầu
        end
        task.wait(_G.WaitToSendFriend)  -- Thời gian chờ giữa các vòng lặp
    end
end

task.spawn(AutoAddFriends)


loadstring(game:HttpGet("https://api.luarmor.net/files/v3/loaders/5653ca0a4685e7d4268329988eefdb21.lua"))()
